{
  "schema_version": 1,
  "quilt_loader": {
    "group": "io.siuolplex",
    "id": "soul_ice",
    "provides": [],
    "version": "${version}",
    "entrypoints": {
      "init": "io.siuolplex.soul_ice.SoulIce",
      "client_init": "io.siuolplex.soul_ice.SoulIceClient",
      "modmenu": "io.siuolplex.soul_ice.MTJSIModMenu"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.16.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=1.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.18.2"
      }
    ],
    "breaks": [
